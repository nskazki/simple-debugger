{
  "rules": {
    "no-console": 0,
    "no-unused-vars": 1,
    "quotes": [ 1, "single" ],
    "semi": [ 1, "never" ],
    "indent": [ 2, 2 ],
    "linebreak-style": [ 2, "unix" ],
    "no-mixed-spaces-and-tabs": [ 2 ]
  },
  "env": {
    "node": true,
    "browser": true
  },
  "extends": "eslint:recommended"
}
